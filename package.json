{
  "name": "taylorbryant.blog",
  "description": "A blog about front-end development by Taylor Bryant",
  "version": "3.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/react-fontawesome": "0.0.18",
    "gatsby": "^1.9.241",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-source-filesystem": "^1.5.27",
    "gatsby-transformer-remark": "^1.7.39",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "taylor",
    "bryant",
    "front-end",
    "developer",
    "html",
    "css",
    "javascript",
    "gatsby",
    "react"
  ],
  "license": "MIT",
  "scripts": {
    "build":
      "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && purgecss -c purgecss.config.js -o ./src/layouts && gatsby build",
    "develop":
      "tailwind build ./src/layouts/index.tailwind.css -c ./tailwind.js -o ./src/layouts/index.css && gatsby develop",
    "format": "prettier --write 'src/**/*.js'"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "purgecss": "^0.21.0",
    "tailwindcss": "^0.5.1"
  }
}
